              {
                "accountId": 3520840,
                "query": "FROM Span SELECT count(*) as 'Total transactions', average(duration.ms) as 'Avg duration (ms)', percentile(duration.ms, 99) as 'Slowest 1% (ms)', percentage(count(*), WHERE otel.status_code != 'ERROR') AS 'Success rate' SINCE 1 WEEK AGO WHERE (span.kind LIKE 'server' OR span.kind LIKE 'consumer' OR kind LIKE 'server' OR kind LIKE 'consumer') AND service.name LIKE 'vaadin%'"
