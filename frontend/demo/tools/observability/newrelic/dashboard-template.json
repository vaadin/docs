        {
          "title": "CPU Utilization",
          "layout": {
            "column": 2,
            "row": 6,
            "width": 5,
            "height": 3
          },
          "linkedEntityGuids": null,
          "visualization": {
            "id": "viz.line"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountId": 3520840,
                "query": "SELECT max(process.runtime.jvm.cpu.utilization) * 100 as cpuUsage FROM Metric FACET service.instance.id SINCE 30 minutes ago TIMESERIES"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            },
            "yAxisLeft": {
              "zero": true
            }
          }
        },
        {
          "title": "Heap memory usage",
          "layout": {
            "column": 7,
            "row": 6,
            "width": 6,
            "height": 3
          },
          "linkedEntityGuids": null,
          "visualization": {
            "id": "viz.line"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountId": 3520840,
                "query": "SELECT sum(used) as Used,sum(committed) as Committed FROM (\n              SELECT average(process.runtime.jvm.memory.usage) as used,average(process.runtime.jvm.memory.committed) as committed FROM Metric WHERE (service.name LIKE 'vaadin%') AND (type = 'heap') FACET `pool`, `type`, `service.instance.id` TIMESERIES) FACET service.instance.id SINCE 30 minutes ago TIMESERIES"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            },
            "yAxisLeft": {
              "zero": true
            }
          }
        }
      ]
